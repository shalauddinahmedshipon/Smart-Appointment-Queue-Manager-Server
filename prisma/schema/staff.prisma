model Staff {
  id            String        @id @default(uuid())
  name          String
  serviceType   String
  dailyCapacity Int           @default(5)
  status        StaffStatus   @default(AVAILABLE)
  accountId     String
  appointments  Appointment[]
  account       Account       @relation(fields: [accountId], references: [id])
}

enum StaffStatus {
  AVAILABLE
  ON_LEAVE
}

enum StaffType {
  DOCTOR
  CONSULTANT
  SUPPORT_AGENT
}

enum ServiceDuration {
  MIN_15
  MIN_30
  MIN_60
}
